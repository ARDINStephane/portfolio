<div id="carouselExampleIndicators" class="carousel slide perso-degraded-backgroung" data-ride="carousel">
    <ol class="carousel-indicators">
        <li data-target="#carouselExampleIndicators" class="active bg-white" data-slide-to="0"></li>
        <li data-target="#carouselExampleIndicators" class="bg-white" data-slide-to="1"></li>
        <li data-target="#carouselExampleIndicators" class="bg-white" data-slide-to="2"></li>
    </ol>
    <div class="carousel-inner">
    {% set x = 0 %}
        {% for project in  projectList %}
            <div class="carousel-item {% if x == 0 %}active{% endif %}">
                <img class="d-block w-50 h-50 mx-auto img-thumbnail border border-dark" src="{{ vich_uploader_asset(project, 'imageFile') }}" alt="slide {{ x }}">
            </div>
            {% set x = x + 1 %}
        {% endfor %}
    </div>
    <a class="carousel-control-prev" href="#carouselExampleIndicators" role="button" data-slide="prev">
        <span class="carousel-control-prev-icon" aria-hidden="true"></span>
        <span class="sr-only">Previous</span>
    </a>
    <a class="carousel-control-next" href="#carouselExampleIndicators" role="button" data-slide="next">
        <span class="carousel-control-next-icon" aria-hidden="true"></span>
        <span class="sr-only">Next</span>
    </a>
</div>